{
  "name":"Linkedin connection ",
  "version":"1.0",
  "description":"LinkBank",
  "author":"Mayank Thakur",
  "manifest_version":2,
  "browser_action":{
    "default_icon":"linkedin.png"
  },
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["content.js"]
    
    }
  ],
  
  "icons":{
    "16":"linkedin.png",
    "32":"linkedin.png",
    "64":"linkedin.png",
    "128":"linkedin.png"
  },
  "permissions":[
    "activeTab",
    "<all_urls>",
    "tabs"
  ],
  "background":{
    "scripts":["background.js"],
    "persistent":false
  }
}